al;\n                font-weight: 700;\n                line-height: 145.9%;\n                font-size: 12px;\n                div{\n                    position: absolute;\n                    width:100%;\n                    margin:0 auto;\n                    height: 100%;\n                    top:0;\n                    left:0;\n                    display: flex;\n                    align-items: center;\n                    justify-content: center;\n\n                }\n                img{\n\n                    object-fit: contain;\n                }\n                .outline{\n                    width:100%;\n                    height: 100%;\n                    \n                }\n                .bg{\n                    width:100%;\n                    position: absolute;\n                    top:0;\n                    left:0;\n                    height: 100%;\n                    object-fit: cover;\n                    font-family: 'Brinnan';\n                    font-style: normal;\n                    font-weight: 700;\n                    font-size: 11px;\n                    line-height: 145.9%;\n                    letter-spacing: 0.1em;\n                    color: #36104C;\n                }\n            }\n            .s-btn:hover{\n                .bg{\n                    transform: translate3d(-5.23px, -5.27px, -5.27px);\n                }\n                div{\n                    transform: translate3d(-5.23px, -5.27px, -5.27px);\n                }\n            }\n            .s-btn:active{\n                .bg{\n                    transform: translate3d(0px, 0px, 0px);\n                }\n                div{\n                    transform: translate3d(0px, 0px, 0px);\n                }\n            }\n        }\n    }\n</style>\n<style lang=\"scss\" scoped>\n    @media (min-width: 768px) {\n        .fourth{\n            display: flex;\n            justify-content: center;\n          .left{\n            // margin-right: 100px;\n       